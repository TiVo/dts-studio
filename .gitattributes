# Set the default behavior, in case people don't have core.autocrlf set.
* text=auto

# Explicitly declare text files you want to always be normalized and converted
# to native line endings on checkout.
# The following filter would make all the C sources be normalized:
# *.c text
# We would like the C, C++, Makefile, CMakeLists.txt and other files have Linux line endings.

# Declare files that will always have Windows (CRLF) line endings on checkout.
*.sln          text eol=crlf
*.vcxproj      text eol=crlf
*.filters      text eol=crlf
*.vcxproj.user text eol=crlf
*.bat          text eol=crlf
*.appxmanifest text eol=crlf
*.csproj       text eol=crlf
*.def          text eol=crlf
*.idl          text eol=crlf
*.jsproj       text eol=crlf
*.rc           text eol=crlf
*.RC           text eol=crlf
*.rgs          text eol=crlf


# Set default behavior for command prompt diff.
*.cs diff=csharp

# Declare files that will always have Linux (LF) line endings on checkout.
*.sh text eol=lf

# Denote all files that are truly binary and should go to the LFS
*.JPG                 filter=lfs diff=lfs merge=lfs -text
*.SFX                 filter=lfs diff=lfs merge=lfs -text
*.a                   filter=lfs diff=lfs merge=lfs -text
*.audio               filter=lfs diff=lfs merge=lfs -text
*.avi                 filter=lfs diff=lfs merge=lfs -text
*.bin                 filter=lfs diff=lfs merge=lfs -text
*.binarypb            filter=lfs diff=lfs merge=lfs -text
*.bmp                 filter=lfs diff=lfs merge=lfs -text
*.cat                 filter=lfs diff=lfs merge=lfs -text
*.chkpt-0             filter=lfs diff=lfs merge=lfs -text
*.chm                 filter=lfs diff=lfs merge=lfs -text
*.cur                 filter=lfs diff=lfs merge=lfs -text
*.data-00000-of-00001 filter=lfs diff=lfs merge=lfs -text
*.devicemetadata-ms   filter=lfs diff=lfs merge=lfs -text
*.dll                 filter=lfs diff=lfs merge=lfs -text
*.doc                 filter=lfs diff=lfs merge=lfs -text
*.docx                filter=lfs diff=lfs merge=lfs -text
*.exe                 filter=lfs diff=lfs merge=lfs -text
*.fbx                 filter=lfs diff=lfs merge=lfs -text
*.gif                 filter=lfs diff=lfs merge=lfs -text
*.h264                filter=lfs diff=lfs merge=lfs -text
*.icc                 filter=lfs diff=lfs merge=lfs -text
*.ico                 filter=lfs diff=lfs merge=lfs -text
*.ICO                 filter=lfs diff=lfs merge=lfs -text
*.index               filter=lfs diff=lfs merge=lfs -text
*.ivf                 filter=lfs diff=lfs merge=lfs -text
*.jar                 filter=lfs diff=lfs merge=lfs -text
*.jpg                 filter=lfs diff=lfs merge=lfs -text
*.jsv                 filter=lfs diff=lfs merge=lfs -text
*.lib                 filter=lfs diff=lfs merge=lfs -text
*.meta                filter=lfs diff=lfs merge=lfs -text
*.mpeg2               filter=lfs diff=lfs merge=lfs -text
*.mpp                 filter=lfs diff=lfs merge=lfs -text
*.mp4                 filter=lfs diff=lfs merge=lfs -text
*.msi                 filter=lfs diff=lfs merge=lfs -text
*.nupkg               filter=lfs diff=lfs merge=lfs -text
*.o                   filter=lfs diff=lfs merge=lfs -text
*.obj                 filter=lfs diff=lfs merge=lfs -text
*.odg                 filter=lfs diff=lfs merge=lfs -text
*.odt                 filter=lfs diff=lfs merge=lfs -text
*.oxps                filter=lfs diff=lfs merge=lfs -text
*.p7s                 filter=lfs diff=lfs merge=lfs -text
*.pb                  filter=lfs diff=lfs merge=lfs -text
*.pdb                 filter=lfs diff=lfs merge=lfs -text
*.pdf                 filter=lfs diff=lfs merge=lfs -text
*.png                 filter=lfs diff=lfs merge=lfs -text
*.pngblob             filter=lfs diff=lfs merge=lfs -text
*.ppm                 filter=lfs diff=lfs merge=lfs -text
*.ppt                 filter=lfs diff=lfs merge=lfs -text
*.pptx                filter=lfs diff=lfs merge=lfs -text
*.raw                 filter=lfs diff=lfs merge=lfs -text
*.so                  filter=lfs diff=lfs merge=lfs -text
*.sys                 filter=lfs diff=lfs merge=lfs -text
*.tflite              filter=lfs diff=lfs merge=lfs -text
*.uuu                 filter=lfs diff=lfs merge=lfs -text
*.video               filter=lfs diff=lfs merge=lfs -text
*.wav                 filter=lfs diff=lfs merge=lfs -text
*.winmd               filter=lfs diff=lfs merge=lfs -text
*.wma                 filter=lfs diff=lfs merge=lfs -text
*.wmv                 filter=lfs diff=lfs merge=lfs -text
*.xls                 filter=lfs diff=lfs merge=lfs -text
*.xlsx                filter=lfs diff=lfs merge=lfs -text
*.xps                 filter=lfs diff=lfs merge=lfs -text
*.yuv                 filter=lfs diff=lfs merge=lfs -text
*.zip                 filter=lfs diff=lfs merge=lfs -text
